  "description": "A simple OpenAI app that teaches you how to say hello in multiple languages",
  "main": "app.ts",
  "type": "module",
  "scripts": {
    "dev": "tsx watch app.ts",
    "start": "tsx app.ts",
    "server": "tsx server.ts",
